<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrunal.admincollegeapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdminCollegeApp">
        <activity android:name="com.mrunal.admincollegeapp.notice.DeleteNoticeActivity"></activity>
        <activity android:name="com.mrunal.admincollegeapp.faculty.UpdateTeacherActivity" />
        <activity android:name="com.mrunal.admincollegeapp.faculty.AddTeacher" />
        <activity android:name="com.mrunal.admincollegeapp.faculty.UpdateFaculty" />
        <activity android:name="com.mrunal.admincollegeapp.UploadPdfActivity" />
        <activity android:name="com.mrunal.admincollegeapp.UploadImage" />
        <activity android:name="com.mrunal.admincollegeapp.notice.UploadNotice" />
        <activity android:name="com.mrunal.admincollegeapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>